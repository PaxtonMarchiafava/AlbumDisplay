void setup () {

  pinMode(OE, OUTPUT);
  pinMode(LAT, OUTPUT);
  pinMode(CLK, OUTPUT);
  
  digitalWrite(OE, HIGH);
  digitalWrite(LAT, LOW);
  digitalWrite(CLK, LOW);
  
  for (byte i = 0; i < (sizeof(ABCD) / sizeof(ABCD[0])); i++) {
    pinMode(ABCD[i], OUTPUT);
    digitalWrite(ABCD[i], LOW);
  }
  
  for (byte i = 0; i < (sizeof(RGB1) / sizeof(RGB1[0])); i++) {
    pinMode(RGB1[i], OUTPUT);
    pinMode(RGB2[i], OUTPUT);
    digitalWrite(RGB1[i], LOW);
    digitalWrite(RGB2[i], LOW);
  }
  
  xTaskCreatePinnedToCore(
    display, // function
    "DisplayTask", // task name
    10000, // stack size allocated for task
    NULL, // parameters
    1, // task priority
    &Task1, // task handle to keep track of task
    0); // core assignment           
    
    
  Serial.begin(115200);
    
    
    
}

void loop () {
    
  if (Serial.available()) {
    char a = Serial.read();

    if (a != 'k') {  update(a);  }
    if (a == 'k') {  reset();  }
    
    a = NULL;
  }

}

void drawImage () {

  for (byte y = 0; y < 16; y++) { // for every line

    digitalWrite(OE, HIGH);
    digitalWrite(ABCD[3], (y & (1 << 3)));
    digitalWrite(ABCD[2], (y & (1 << 2)));
    digitalWrite(ABCD[1], (y & (1 << 1)));
    digitalWrite(ABCD[0], (y & (1 << 0)));



    for (byte x = 0; x < 8; x++) { // top panel
      for (byte bit = 0; bit < 8; bit++) {
        digitalWrite(RGB2[0], (imageData[0] [15 - y] [7 - x] & (1 << bit))); // top half of panel
        digitalWrite(RGB2[1], (imageData[1] [15 - y] [7 - x] & (1 << bit)));
        digitalWrite(RGB2[2], (imageData[2] [15 - y] [7 - x] & (1 << bit)));
        
        digitalWrite(RGB1[0], (imageData[0] [31 - y] [7 - x] & (1 << bit))); // bottom half of panel
        digitalWrite(RGB1[1], (imageData[1] [31 - y] [7 - x] & (1 << bit)));
        digitalWrite(RGB1[2], (imageData[2] [31 - y] [7 - x] & (1 << bit)));

        digitalWrite(CLK, HIGH);
        digitalWrite(CLK, LOW);
      }
    }
    
    for (byte x = 0; x < 8; x++) { // bottom panel
      for (byte bit = 0; bit < 8; bit++) {
        digitalWrite(RGB1[0], (imageData[0] [y + 32] [x] & (1 << 7 - bit)));
        digitalWrite(RGB1[1], (imageData[1] [y + 32] [x] & (1 << 7 - bit)));
        digitalWrite(RGB1[2], (imageData[2] [y + 32] [x] & (1 << 7 - bit)));
        
        digitalWrite(RGB2[0], (imageData[0] [y + 16 + 32] [x] & (1 << 7 - bit)));
        digitalWrite(RGB2[1], (imageData[1] [y + 16 + 32] [x] & (1 << 7 - bit)));
        digitalWrite(RGB2[2], (imageData[2] [y + 16 + 32] [x] & (1 << 7 - bit)));

        digitalWrite(CLK, HIGH);
        digitalWrite(CLK, LOW);
      }
    }





    
    digitalWrite(LAT, HIGH);
    digitalWrite(LAT, LOW);
    digitalWrite(OE, LOW);
    
    delayMicroseconds(200);
  }
  
  // delay(500);

}

void display ( void * pvParameters ) { // core task to display forever
  Serial.print("Display task running on core ");
  Serial.println(xPortGetCoreID());

  for (;;) {
    drawImage();
    vTaskDelay(1); // pdMS_TO_TICKS(1)
  }
}

void update (byte a) {
  reset();
  
  String testImage = ""; // image stored in a string created by BuildArray.py for testing

  switch (a) {
    case 'a':
      testImage = "";
      break;
    case 'b':
      testImage = "";
      break;
    case 'c':
      testImage = "";
      break;
    case 'd':
      testImage = "";
      break;
  }
  
  
  
  for (byte x = 0; x < 64; x++) {
    for (byte y = 0; y < 32; y++) {
      if (testImage.charAt( (x*32*6) + (y*6) ) == '1') { // r
        imageData[0][y][(int) floor(x/8)] = imageData[0][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
      }

      if (testImage.charAt( (x*32*6) + (y*6) + 2) == '1') { // g
        imageData[1][y][(int) floor(x/8)] = imageData[1][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
      }
      
      if (testImage.charAt( (x*32*6) + (y*6) + 4) == '1') { // b
        imageData[2][y][(int) floor(x/8)] = imageData[2][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
      }
    }
  }
}

void reset () {
  Serial.println("we in");
  for (byte j = 0; j < 3; j++) {
    for (byte k = 0; k < 64; k++) {
      for (byte l = 0; l < 8; l++) {
        imageData[j][k][l] = 0;
      }
    }
  }
}