void setup () {

  pinMode(OE, OUTPUT);
  pinMode(LAT, OUTPUT);
  pinMode(CLK, OUTPUT);
  
  digitalWrite(OE, HIGH);
  digitalWrite(LAT, LOW);
  digitalWrite(CLK, LOW);
  
  for (byte i = 0; i < (sizeof(ABCD) / sizeof(ABCD[0])); i++) {
    pinMode(ABCD[i], OUTPUT);
    digitalWrite(ABCD[i], LOW);
  }
  
  for (byte i = 0; i < (sizeof(RGB1) / sizeof(RGB1[0])); i++) {
    pinMode(RGB1[i], OUTPUT);
    pinMode(RGB2[i], OUTPUT);
    digitalWrite(RGB1[i], LOW);
    digitalWrite(RGB2[i], LOW);
  }
  
  xTaskCreatePinnedToCore(
    display, // function
    "DisplayTask", // task name
    10000, // stack size allocated for task
    NULL, // parameters
    1, // task priority
    &Task1, // task handle to keep track of task
    0); // core assignment           
    
    
  Serial.begin(115200);
    
    
    
  }
  
  
  
void loop () {
    
  if (Serial.available()) {
    char a = Serial.read();

    if (a != 'k') {  update(a);  }
    if (a == 'k') {  reset();  }
    
    a = NULL;
  }

}

void drawImage () {

  for (byte l = 0; l < 2; l++) {
    for (byte i = 0; i < 16; i++) { // for every line
  
      digitalWrite(OE, HIGH);
      digitalWrite(ABCD[3], (i & (1 << 3)));
      digitalWrite(ABCD[2], (i & (1 << 2)));
      digitalWrite(ABCD[1], (i & (1 << 1)));
      digitalWrite(ABCD[0], (i & (1 << 0)));
  
  
      for (byte j = 0; j < 8 ; j++) { // for all bytes in line (sizeof(imageData[0][0]) / sizeof(imageData[0][0][0]))
        for (byte k = 0; k < 8; k++) { // for all bits in byte sizeof(imageData[0][0][0])
          
          digitalWrite(RGB1[0], (imageData[l][0][i][j] & (1 << 7 - k)));
          digitalWrite(RGB1[1], (imageData[l][1][i][j] & (1 << 7 - k)));
          digitalWrite(RGB1[2], (imageData[l][2][i][j] & (1 << 7 - k)));
          
          digitalWrite(RGB2[0], (imageData[l][0][i + 16][j] & (1 << 7 - k)));
          digitalWrite(RGB2[1], (imageData[l][1][i + 16][j] & (1 << 7 - k)));
          digitalWrite(RGB2[2], (imageData[l][2][i + 16][j] & (1 << 7 - k)));
  
          digitalWrite(CLK, HIGH);
          digitalWrite(CLK, LOW);
        }
      }
      digitalWrite(LAT, HIGH);
      digitalWrite(LAT, LOW);
      digitalWrite(OE, LOW);

      delayMicroseconds(200);
    }
  }
  

}

void display ( void * pvParameters ) { // core task to display forever
  Serial.print("Display task running on core ");
  Serial.println(xPortGetCoreID());

  for (;;) {
    drawImage();
    vTaskDelay(1); // pdMS_TO_TICKS(1)
  }
}

void update (byte a) {
  // byte imageData [2][3][32][8] = { // brightness, rgb, y, x
  reset();
  
  String testImage = ""; // image stored in a string created by BuildArray.py for testing

  switch (a) {
    case 'a':
      testImage = "";
      break;
    case 'b':
      testImage = "";
      break;
    case 'c':
      testImage = "";
      break;
    case 'd':
      testImage = "";
      break;
  }
  
  
  
  for (byte x = 0; x < 64; x++) {
    for (byte y = 0; y < 32; y++) {
      if (testImage.charAt( (x*32*6) + (y*6) ) == '1') { // r
        imageData[0][0][y][(int) floor(x/8)] = imageData[0][0][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[0][0][y][(int) floor(x/8)] = 0;
      }
      if (testImage.charAt( (x*32*6) + (y*6) + 1) == '1') {
        imageData[1][0][y][(int) floor(x/8)] = imageData[1][0][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[1][0][y][(int) floor(x/8)] = 0;
      }
      if (testImage.charAt( (x*32*6) + (y*6) + 2) == '1') { // g
        imageData[0][1][y][(int) floor(x/8)] = imageData[0][1][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[0][1][y][(int) floor(x/8)] = 0;
      }
      if (testImage.charAt( (x*32*6) + (y*6) + 3) == '1') {
        imageData[1][1][y][(int) floor(x/8)] = imageData[1][1][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[1][1][y][(int) floor(x/8)] = 0;
      }
      if (testImage.charAt( (x*32*6) + (y*6) + 4) == '1') { // b
        imageData[0][2][y][(int) floor(x/8)] = imageData[0][2][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[0][2][y][(int) floor(x/8)] = 1;
      }
      if (testImage.charAt( (x*32*6) + (y*6) + 5) == '1') {
        imageData[1][2][y][(int) floor(x/8)] = imageData[1][2][y][(int) floor(x/8)] | (1 << (7 - (x%8)));
        // imageData[1][2][y][(int) floor(x/8)] = 1;
      }
    }
  }
}

void reset () {
  Serial.println("we in");
  for (byte i = 0; i < 2; i++) {
    for (byte j = 0; j < 3; j++) {
      for (byte k = 0; k < 32; k++) {
        for (byte l = 0; l < 8; l++) {
          imageData[i][j][k][l] = 0;
        }
      }
    }
  }
}